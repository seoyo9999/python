fruit = ['사과', '배', '감', '포도']
fruit.append('딸기')
if '딸기' in fruit : #if '딸기' not in fruit :
  print("딸기가 있습니다.")
  print("딸기가 없습니다.")
print(fruit)

//프린트 값: ['사과', '배', '감', '포도', '딸기']

